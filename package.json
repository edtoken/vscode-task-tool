{
    "name": "vscode-task-tool",
    "displayName": "TaskTool",
    "description": "TaskTool issue list and create for VSCode.",
    "version": "0.0.0",
    "publisher": "edtoken",
    "engines": {
        "vscode": "^1.15.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/edtoken/vscode-task-tool"
    },
    "icon": "icon.png",
    "author": {
        "name": "edtoken",
        "url": "https://www.edtoken.github.io",
        "email": "editied@gmail.com"
    },
    "keywords": [
        "youtrack",
        "jira",
        "issue",
        "task",
        "edtoken"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "tasktool.test-connection",
                "title": "TaskTool: Test connection"
            },
            {
                "command": "tasktool.list",
                "title": "TaskTool: List"
            },
            {
                "command": "tasktool.open",
                "title": "TaskTool: Open Issue"
            },
            {
                "command": "tasktool.close",
                "title": "TaskTool: Close Active Issue"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "@types/xml2js": "^0.4.0",
        "@types/pug": "^2.0.4",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "https": "^1.0.0",
        "pug": "^2.0.0-rc.3",
        "xml2js": "^0.4.19"
    }
}
